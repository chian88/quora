.container.py-3
  .row
    .col-3
      %ul
        %li
          %h6 Feeds
        %hr/
        %li
          %a{:href => "#"} Top Stories
        %li
          %a{:href => "#"} New Stories
    .col-7
      %h5 Topics you are interested asked by others.
      %hr
      - @topics.each do |topic|
        - if topic.questions.any?{ |q| q.user != current_user }
          %hr
          - topic.questions.each do |question|
            - next if question.user == current_user
            = render 'question', question: question, topic: topic
